{
  "plugins": [ "import", "jsx-a11y", "react" ],
  "extends": [
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:jsx-a11y/recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "es6": true,
    "mongo": true,
    "node": true,
    "serviceworker": true,
    "worker": true,
    "browser": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "arrowFunctions": true,
      "binaryLiterals": true,
      "blockBindings": true,
      "classes": true,
      "defaultParams": true,
      "destructuring": true,
      "experimentalObjectRestSpread": true,
      "forOf": true,
      "generators": true,
      "globalReturn": true,
      "impliedStrict": true,
      "modules": true,
      "objectLiteralComputedProperties": true,
      "objectLiteralDuplicateProperties": true,
      "objectLiteralShorthandMethods": true,
      "objectLiteralShorthandProperties": true,
      "octalLiterals": false,
      "regexUFlag": false,
      "regexYFlag": false,
      "restParams": true,
      "spread": true,
      "superInFunctions": true,
      "templateStrings": true,
      "unicodeCodePointEscapes": false
    }
  },
  "rules": {
    "indent": [
      "warn",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "semi": [ "warn", "always" ],
    "quotes": [ "warn", "single", { "avoidEscape": true, "allowTemplateLiterals": true } ],
    "for-direction": "error",
    "getter-return": "error",
    "no-compare-neg-zero": "error",
    "no-dupe-args": "error",
    "no-dupe-keys": "error",
    "no-duplicate-case": "error",
    "no-empty": "error",
    "no-empty-character-class": "error",
    "no-ex-assign": "error",
    "no-extra-boolean-cast": "off",
    "no-extra-parens": "off",
    "no-extra-semi": "off",
    "no-func-assign": "warn",
    "no-inner-declarations": "off",
    "no-invalid-regexp": "warn",
    "no-misleading-character-class": "warn",
    "no-unreachable": "warn",
    "no-unsafe-finally": "error",
    "no-unsafe-negation": "error",
    "require-atomic-updates": "warn",
    "use-isnan": "warn",
    "valid-typeof": "warn",
    "accessor-pairs": "warn",
    "class-methods-use-this": "off",
    "default-case": "error",
    "dot-location": [ "error", "property" ],
    "no-case-declarations": "warn",
    "no-empty-pattern": "warn",
    "no-floating-decimal": "warn",
    "no-global-assign": "error",
    "no-invalid-this": "off",
    "no-magic-numbers": "off",
    "no-param-reassign": "warn",
    "no-return-assign": "warn",
    "no-return-await": "warn",
    "no-self-assign": "warn",
    "no-self-compare": "warn",
    "no-implied-eval": "error",
    "no-new-wrappers": "warn",
    "no-new-func": "warn",
    "no-throw-literal": "error",
    "no-unmodified-loop-condition": "error",
    "no-useless-escape": "warn",
    "vars-on-top": "warn",
    "wrap-iife": "warn",
    "no-delete-var": "error",
    "no-label-var": "error",
    "global-require": "error",
    "no-buffer-constructor": "error",
    "no-path-concat": "error",
    "no-process-env": "off",
    "no-process-exit": "off",
    "array-bracket-newline": [
      "warn",
      {
        "multiline": true,
        "minItems": 4
      }
    ],
    "array-bracket-spacing": [
      "warn",
      "never",
      {
        "objectsInArrays": true,
        "arraysInArrays": true
      }
    ],
    "array-element-newline": [ "warn", "consistent" ],
    "block-spacing": [ "warn", "always" ],
    "brace-style": [ "warn", "stroustrup", { "allowSingleLine": true } ],
    "camelcase": [
      "warn",
      {
        "properties": "always",
        "allow": [ "^UNSAFE_*" ]
      }
    ],
    "comma-dangle": [
      "warn",
      {
        "arrays": "only-multiline",
        "objects": "only-multiline",
        "imports": "never",
        "exports": "never",
        "functions": "never"
      }
    ],
    "comma-spacing": [ "warn", { "before": false, "after": true } ],
    "comma-style": [ "warn", "last" ],
    "computed-property-spacing": [ "warn", "never" ],
    "consistent-this": [ "error", "that" ],
    "no-multiple-empty-lines": [ "warn", { "max": 4, "maxEOF": 1, "maxBOF": 0 } ],
    "func-call-spacing": [ "warn", "never" ],
    "func-name-matching": [ "warn", "always" ],
    "func-names": [ "warn", "as-needed" ],
    "func-style": "off",
    "function-paren-newline": "off",
    "implicit-arrow-linebreak": "off",
    "jsx-quotes": [ "warn", "prefer-single" ],
    "key-spacing": [ "warn", { "beforeColon": false, "afterColon": true, "mode": "strict" } ],
    "keyword-spacing": [ "warn", { "before": true, "after": true } ],
    "line-comment-position": "off",
    "lines-around-comment": "off",
    "lines-between-class-members": [ "warn", "always", { "exceptAfterSingleLine": true } ],
    "multiline-ternary": "off",
    "new-parens": "error",
    "new-cap": [ "error", { "newIsCap": true } ],
    "newline-per-chained-call": [ "warn", { "ignoreChainWithDepth": 3 } ],
    "no-array-constructor": "error",
    "no-bitwise": [ "error", { "allow": [ "~" ], "int32Hint": true } ],
    "no-lonely-if": "off",
    "no-mixed-operators": "error",
    "no-mixed-spaces-and-tabs": "off",
    "no-negated-condition": "off",
    "no-nested-ternary": "off",
    "no-new-object": "off",
    "no-new-symbol": "off",
    "no-plusplus": "off",
    "no-ternary": "off",
    "no-trailing-spaces": "off",
    "no-underscore-dangle": "off",
    "no-unneeded-ternary": [ "warn", { "defaultAssignment": true } ],
    "no-whitespace-before-property": "warn",
    "nonblock-statement-body-position": [ "warn", "beside", { "overrides": { "while": "below", "for": "below" } } ],
    "object-curly-newline": [
      "warn",
      {
        "ObjectPattern": "never",
        "ImportDeclaration": { "multiline": true, "minProperties": 5 },
        "ExportDeclaration": { "multiline": true, "minProperties": 3 }
      }
    ],
    "object-curly-spacing": [ "warn", "always" ],
    "object-property-newline": [ "warn", { "allowAllPropertiesOnSameLine": true } ],
    "operator-linebreak": [ "warn", "before" ],
    "padded-blocks": "off",
    "prefer-spread": "off",
    "prefer-object-spread": "off",
    "quote-props": [ "warn", "as-needed", { "keywords": true, "numbers": true } ],
    "space-before-blocks": [ "warn", { "functions": "always", "keywords": "always", "classes": "always" } ],
    "space-before-function-paren": [ "warn", { "anonymous": "never", "named": "never", "asyncArrow": "always" } ],
    "space-infix-ops": "off",
    "space-unary-ops": [ "warn", { "words": true, "nonwords": false } ],
    "spaced-comment": "off",
    "switch-colon-spacing": [ "warn", { "after": true, "before": false } ],
    "template-tag-spacing": [ "warn", "never" ],
    "arrow-body-style": [ "warn", "as-needed", { "requireReturnForObjectLiteral": false } ],
    "arrow-parens": [ "warn", "as-needed" ],
    "arrow-spacing": [ "warn", { "before": true, "after": true } ],
    "constructor-super": "off",
    "generator-star-spacing": "off",
    "no-class-assign": "error",
    "no-const-assign": "error",
    "no-dupe-class-members": "error",
    "no-duplicate-imports": "error",
    "no-this-before-super": "error",
    "no-useless-computed-key": "off",
    "no-useless-constructor": "off",
    "no-useless-rename": "warn",
    "no-var": "off",
    "object-shorthand": [ "warn", "always", { "avoidExplicitReturnArrows": true, "avoidQuotes": true } ],
    "prefer-const": "warn",
    "prefer-destructuring": "warn",
    "prefer-rest-params": "warn",
    "prefer-template": "warn",
    "require-yield": "error",
    "rest-spread-spacing": [ "warn", "never" ],
    "sort-imports": "off",
    "template-curly-spacing": "off",
    "yield-star-spacing": "off",
    "import/no-named-as-default": "off",
    "react/boolean-prop-naming": "warn",
    "react/prop-types": "warn",
    "react/no-unescaped-entities": "off",
    "react/display-name": "off",
    "jsx-a11y/no-autofocus": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "jsx-a11y/no-noninteractive-tabindex": "off",
    "jsx-a11y/img-redundant-alt": "off",
    "jsx-a11y/no-static-element-interactions": "off",
    "jsx-a11y/click-events-have-key-events": "off"
  },
  "settings": {}
}
